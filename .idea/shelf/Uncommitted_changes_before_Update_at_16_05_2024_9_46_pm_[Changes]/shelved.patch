<+>@import url('https://fonts.googleapis.com/css2?family=Noto+Sans+Canadian+Aboriginal&display=swap');\r\n@import url('https://fonts.googleapis.com/css2?family=Mukta:wght@200;300;400;500;600;700;800&display=swap');\r\n\r\n:root {\r\n    --blue: #3685fb;\r\n    --md-blue: #2065f0;\r\n    --dark-blue: #152556;\r\n    --white: #f6f6f9;\r\n    --black: #272728;\r\n    --light-mode-background-color: #f5f5f5;\r\n    --light-mode-color: #173278;\r\n    --dark-mode-background-color: #212128;\r\n    --dark-mode-color: #CFD9F3;\r\n    --dark-mode-h-color: #eceff8;\r\n    --text-link-color: #c9ddf9;\r\n}\r\n\r\n* {\r\n    border: none;\r\n    outline: none;\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: 'Noto Sans Canadian Aboriginal', nunito, sans-serif;\r\n    font-size: 18px;\r\n    color: var(--black);\r\n    text-decoration: none;\r\n    list-style: none;\r\n    text-transform: capitalize;\r\n    transition: all 0.3s linear;\r\n}\r\n\r\n*::selection {\r\n    background: var(--dark-blue);\r\n    color: var(--white);\r\n}\r\n\r\nhtml {\r\n    font-size: 80%;\r\n    overflow-x: hidden;\r\n    scroll-padding-top: 6.5rem;\r\n    scroll-behaviour: smooth;\r\n}\r\n\r\nimg {\r\n    display: block;\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\nh1, \r\nh2, \r\nh3, \r\nh4, \r\nh5, \r\nh6, \r\np {\r\n    font-family: 'Noto Sans Canadian Aboriginal', nunito, sans-serif;\r\n    color: var(--dark-blue);\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6 {\r\n    font-weight: 600;\r\n    font-family: 'Noto Sans Canadian Aboriginal', nunito, sans-serif;\r\n    color: var(--dark-blue);\r\n}\r\n\r\np {\r\n    font-size: 1.6rem;\r\n    font-weight: 500;\r\n}\r\n\r\nhr {\r\n    border: 1px solid #8f8e8e7d;\r\n    margin: 5px 0;\r\n}\r\n\r\n.icon {\r\n    padding: 3px;\r\n    color: var(--dark-blue);\r\n    font-size: 2rem;\r\n}\r\n\r\nbody {\r\n    font-family: 'Noto Sans Canadian Aboriginal', nunito, sans-serif;\r\n    margin: 0;\r\n    padding: 0 0 100px 0;\r\n    min-height: 100dvh;\r\n    position: relative;\r\n    background: var(--light-mode-background-color);\r\n    color: var(--light-mode-color);\r\n}\r\n\r\n\r\n/* ============= HEADER STYLES ============== */\r\nheader {\r\n    padding: 10px;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    z-index: 2;\r\n    background: var(--light-mode-background-color);\r\n    transition: all 0.5s ease;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    box-shadow: 0px 8px 13px -15px rgba(0, 0, 0, 0.8);\r\n}\r\n\r\n#header.scrolled {\r\n    box-shadow: 0px 3px 10px -3px rgba(0, 0, 0, 0.421);\r\n}\r\n#header.scrolled .nav_logo {\r\n    height: 50px;\r\n    width: 200px;\r\n}\r\n\r\n.nav_logo {\r\n    height: 70px;\r\n    width: 300px;\r\n}\r\n\r\nnav {\r\n    position: relative;\r\n    line-height: 20px;\r\n    display: flex;\r\n    align-items: center;\r\n    flex: 0.8;\r\n}\r\n\r\n.nav_ul__items {\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 0;\r\n    display: flex;\r\n    gap: 2px;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    flex: 0.5;\r\n}\r\n\r\n\r\n.nav_ul__items > li {\r\n    margin: 0 5px;\r\n}\r\n\r\n\r\n.nav_ul__items a {\r\n    color: var(--dark-blue);\r\n    font-size: 1.3rem;\r\n    padding: 5px 20px;\r\n    font-weight: 600;\r\n    text-transform: uppercase;\r\n    display: inline-flex;\r\n    align-items: center;\r\n    position: relative;\r\n    overflow: hidden;\r\n}\r\n\r\n.nav_ul__items a::before {\r\n    content: \"\";\r\n    position: absolute;\r\n    z-index: -1;\r\n    left: 0;\r\n    right: 100%;\r\n    bottom: 0;\r\n    background: linear-gradient(51deg, rgba(49, 99, 232, 0.6) 29%, rgba(23, 69, 187, 0.8) 64%, rgba(2, 36, 112, 0.9) 100%);\r\n    height: 4px;\r\n    -webkit-transition-property: right;\r\n    transition-property: right;\r\n    -webkit-transition-duration: 0.3s;\r\n    transition-duration: 0.3s;\r\n    -webkit-transition-timing-function: ease-out;\r\n    transition-timing-function: ease-out;\r\n}\r\n\r\n.nav_ul__items a:hover::before,\r\n.nav_ul__items a:focus::before,\r\n.nav_ul__items a:active::before {\r\n    right: 0;\r\n}\r\n\r\n.login-btn {\r\n    padding: 10px;\r\n    margin-left: 50px;\r\n}\r\n\r\n.login_link {\r\n    border: 1px solid var(--dark-blue);\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 3px 10px;\r\n    border-radius: .65rem;\r\n    position: relative;\r\n    overflow: hidden;\r\n    color: var(--dark-blue);\r\n    font-size: 1.3rem;\r\n    font-weight: 600;\r\n    text-transform: uppercase;\r\n    background: transparent;\r\n    z-index: 1;\r\n}\r\n\r\n.login_link::before,\r\n.login_link::after {\r\n    content: '';\r\n    display: block;\r\n    width: 50px;\r\n    height: 50px;\r\n    transform: translate(-50%, -50%);\r\n    position: absolute;\r\n    border-radius: 50%;\r\n    z-index: -1;\r\n    background-color: var(--dark-blue);\r\n    transition: 1s ease;\r\n}\r\n\r\n.login_link::before {\r\n    top: -1em;\r\n    left: -1em;\r\n}\r\n\r\n.login_link::after {\r\n    top: calc(100% + 1em);\r\n    left: calc(100% + 1em);\r\n}\r\n\r\n.login_link:hover::before,\r\n.login_link:hover::after {\r\n    width: 400px;\r\n    height: 400px;\r\n}\r\n\r\n.login_link:hover,\r\n.login_link:hover .icon {\r\n    color: var(--white)\r\n}\r\n\r\n.login_link:active {\r\n    filter: brightness(.8);\r\n}\r\n\r\n.menu-toggle {\r\n    cursor: pointer;\r\n    background: transparent;\r\n    outline: none;\r\n    padding: 5px;\r\n    display: none;\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n}\r\n\r\n.menu-toggle .icon {\r\n    font-size: 3rem;\r\n}\r\n\r\n/* Style for close icon (optional) */\r\n.material-symbols-outlined.close {\r\n    transform: rotate(45deg);\r\n    z-index: 3;\r\n}\r\n\r\n.navbar_container.active {\r\n    right: 0;\r\n}\r\n\r\n/* Style the overlay */\r\n.overlay {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    opacity: 0;\r\n    transition: opacity 0.3s ease;\r\n    z-index: 0;\r\n}\r\n\r\n.overlay.active ~ *{\r\n    opacity: 1;\r\n    z-index: 1;\r\n    filter: blur(8px);\r\n    -webkit-filter: blur(8px);\r\n    background-color: rgba(238, 235, 235, 0.35);\r\n}\r\n\r\n\r\n/* ============ MAIN STYLES ===================== */\r\n.main-section {\r\n    margin: 100px 0 50px 0;\r\n    padding: 0;\r\n}\r\n\r\n/* =============== HEADLINE BANNER STYLES =============== */\r\n.header-banner {\r\n    background: url(\"/img/background-2.png\");\r\n}\r\n.course-banner {\r\n    background: url(\"/img/background-4.png\");\r\n}\r\n.detail-banner {\r\n    background: url(\"/img/background-6.png\");\r\n}\r\n.banner {\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    position: relative;\r\n    height: 430px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    padding: 60px 10px 0 10px;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.banner .text {\r\n    position: relative;\r\n    width: 600px;\r\n    background: transparent;\r\n    right: 120px;\r\n    height: fit-content;\r\n}\r\n.banner .text h3{\r\n    line-height: 4rem;\r\n    color: var(--dark-blue);\r\n    margin: 0 0 10px 0;\r\n}\r\n\r\n.banner .text p {\r\n    font-size: 1.4rem;\r\n    text-transform: initial;\r\n    line-height: 25px;\r\n    text-align: justify;\r\n    color: var(--dark-blue);\r\n}\r\n\r\n.headline-btn {\r\n    margin-top: 10px;\r\n    padding: 5px;\r\n    text-align: center;\r\n}\r\n\r\n.register-btn {\r\n    font-size: 1.2rem;\r\n    padding: 1em 3.3em;\r\n    cursor: pointer;\r\n    transform: perspective(200px) rotateX(15deg);\r\n    color: white;\r\n    font-weight: 900;\r\n    border: none;\r\n    border-radius: 5px;\r\n    background: linear-gradient(0deg,\r\n            rgba(63, 94, 251, 1) 0%,\r\n            rgba(70, 135, 252, 1) 100%);\r\n    box-shadow: rgba(63, 94, 251, 0.2) 0px 30px 29px 0px;\r\n    will-change: transform;\r\n    transition: all 0.3s;\r\n    border-bottom: 2px solid rgba(70, 135, 252, 1);\r\n    text-align: center;\r\n}\r\n\r\n.register-btn:hover {\r\n    transform: perspective(180px) rotateX(30deg) translateY(2px);\r\n}\r\n\r\n.register-btn:active {\r\n    transform: perspective(170px) rotateX(36deg) translateY(5px);\r\n}\r\n\r\n\r\n/* ============ Top Courses section styles ==== */\r\n.top-courses {\r\n    background: transparent;\r\n    margin: 30px 0;\r\n}\r\n.top-courses h3 {\r\n    color: #173278;\r\n    text-align: center;\r\n    font-size: 2rem;\r\n    font-weight: 600;\r\n    margin-top: 30px;\r\n}\r\n@keyframes slide {\r\n    from {transform: translateX(0);}\r\n    to {transform: translateX(calc(-250px * 7));}\r\n}\r\n.slider {\r\n    overflow: hidden;\r\n    position: relative;\r\n    width: 90%;\r\n    height: 100%;\r\n    margin: auto;\r\n    padding: 40px 0;\r\n}\r\n.slider::before,\r\n.slider::after {\r\n    position: absolute;\r\n    top: -1px;\r\n    width: 120px;\r\n    height: 100%;\r\n    content: \"\";\r\n    z-index: 1;\r\n}\r\n.slider::before {\r\n    left: 0;\r\n    background: linear-gradient(to left, rgba(245, 245, 245, 0), rgba(245, 245, 245, 0.8));\r\n}\r\n.slider::after {\r\n    right: 0;\r\n    background: linear-gradient(to right, rgba(245, 245, 245, 0), rgb(245, 245, 245, 0.8));\r\n}\r\n.slide-track {\r\n    white-space: no-wrap;\r\n    display: flex;\r\n    gap: 10px;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: calc(250px * 14);\r\n    animation: 15s slide infinite linear;\r\n    transform: translateX(-100%);\r\n}\r\n\r\n.slide-track img {\r\n    height: 70px;\r\n    width: 100%;\r\n    margin: auto;\r\n    transition: transform 0.5s ease-in-out;\r\n    cursor: pointer;\r\n    object-fit: contain;\r\n    aspect-ratio: 1;\r\n}\r\n\r\n.slide-track img:hover {\r\n    transform: scale(1.1);\r\n}\r\n\r\n/* ================= Why Us ================== */\r\n.why-us {\r\n    padding: 0 10px 30px 10px