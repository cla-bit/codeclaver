solid #1850dd;\r\n}\r\n\r\n.course-btn .see-more:hover:before {\r\n    top: -35%;\r\n    background-color: #1850dd;\r\n    transform: translateX(-50%) scaleY(1.3) scaleX(0.8);\r\n}\r\n\r\n.course-btn .see-more:hover:after {\r\n    top: -45%;\r\n    background-color: #1850dd;\r\n    transform: translateX(-50%) scaleY(1.3) scaleX(0.8);\r\n}\r\n\r\n/* =============== Our pdf ============= */\r\n.our-pdf {\r\n    margin-top: 30px;\r\n    background: url(\"/img/magicpattern-grid-pattern-1715351392430.png\");\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\r\n    gap: 20px;\r\n}\r\n\r\n.our-pdf .text h4 {\r\n    line-height: 70px;\r\n    font-size: 4rem;\r\n    margin: 60px 50px;\r\n    display: inline-flex;\r\n    flex-direction: column;\r\n    color: var(--white);\r\n}\r\n\r\n.our-pdf .text h4 span{\r\n\r\n    font-size: 6rem;\r\n    color: var(--white);\r\n}\r\n\r\n.our-pdf .text p {\r\n    font-size: 2rem;\r\n    margin: 0 50px;\r\n    color: var(--white);\r\n}\r\n.our-pdf .our-pdf-image {\r\n    position: relative;\r\n    cursor: pointer;\r\n    overflow: hidden;\r\n}\r\n\r\n.our-pdf .our-pdf-image .image:hover {\r\n    -webkit-animation: heartbeat 1.5s ease-in-out infinite both;\r\n    animation: heartbeat 1.5s ease-in-out infinite both;\r\n    transition: all 0.3s ease-in-out;\r\n}\r\n\r\n@-webkit-keyframes heartbeat {\r\n    from {\r\n        -webkit-transform: scale(1);\r\n        transform: scale(1);\r\n        -webkit-transform-origin: center center;\r\n        transform-origin: center center;\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n\r\n    10% {\r\n        -webkit-transform: scale(0.91);\r\n        transform: scale(0.91);\r\n        -webkit-animation-timing-function: ease-in;\r\n        animation-timing-function: ease-in;\r\n    }\r\n\r\n    17% {\r\n        -webkit-transform: scale(0.98);\r\n        transform: scale(0.98);\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n\r\n    33% {\r\n        -webkit-transform: scale(0.87);\r\n        transform: scale(0.87);\r\n        -webkit-animation-timing-function: ease-in;\r\n        animation-timing-function: ease-in;\r\n    }\r\n\r\n    45% {\r\n        -webkit-transform: scale(1);\r\n        transform: scale(1);\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n}\r\n\r\n@keyframes heartbeat {\r\n    from {\r\n        -webkit-transform: scale(1);\r\n        transform: scale(1);\r\n        -webkit-transform-origin: center center;\r\n        transform-origin: center center;\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n\r\n    10% {\r\n        -webkit-transform: scale(0.91);\r\n        transform: scale(0.91);\r\n        -webkit-animation-timing-function: ease-in;\r\n        animation-timing-function: ease-in;\r\n    }\r\n\r\n    17% {\r\n        -webkit-transform: scale(0.98);\r\n        transform: scale(0.98);\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n\r\n    33% {\r\n        -webkit-transform: scale(0.87);\r\n        transform: scale(0.87);\r\n        -webkit-animation-timing-function: ease-in;\r\n        animation-timing-function: ease-in;\r\n    }\r\n\r\n    45% {\r\n        -webkit-transform: scale(1);\r\n        transform: scale(1);\r\n        -webkit-animation-timing-function: ease-out;\r\n        animation-timing-function: ease-out;\r\n    }\r\n}\r\n\r\n/* =========== SUBSCRIBE STYLES ======== */\r\n.subscribe-container {\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n}\r\n\r\n.subscribe-form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    background: transparent;\r\n    padding: 20px;\r\n    border-radius: 10px;\r\n    width: 100%;\r\n}\r\n\r\n.subscribe-title {\r\n 